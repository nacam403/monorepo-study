{
  "name": "my-common",
  "version": "1.0.0",
  "main": "dist/index.js",
  "private": true,
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "upbin npm-run-all clean build:*",
    "build:tsc": "upbin tsc",
    "build:babel": "upbin babel src --extensions '.ts,.tsx' --out-dir dist --root-mode upward --source-maps",
    "test": "upbin mocha --recursive 'test/**/*.test.ts' --require ../../test/babel-register.js",
    "lint": "upbin eslint . --ext .ts,.tsx"
  },
  "devDependencies": {
    "upbin": "^0.8.1"
  }
}
